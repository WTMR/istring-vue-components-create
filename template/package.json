{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  "author": {
    "name": "{{ authorFullNameFrom author }}",
    "email": "{{ authorEmailFrom author }}"
  },
  "keywords": [
    "vue",
    "vuejs",
    "components"
  ],
  "license": "MIT",
  "main": "dist/{{ name }}.js",
  "module": "index.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js --progress --hide-modules"
  },
  {{#unless_eq githubAccount ""}}
  "repository": {
    "type": "git",
    "url": "git+http://202.109.200.36:6280/{{ githubAccount }}/{{ name }}.git"
  },
  "bugs": {
    "url": "http://202.109.200.36:6280/{{ githubAccount }}/{{ name }}/issues"
  },
  "homepage": "http://202.109.200.36:6280/{{ githubAccount }}/{{ name }}#readme",
  {{/unless_eq}}
  "dependencies": {
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "webpack-bundle-analyzer": "^2.2.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.2.0",
    "rimraf": "^2.6.0",
    "url-loader": "^0.5.8",
    "file-loader": "^0.11.1",
    "json-loader": "^0.5.4",
    "style-loader": "0.13.1",
    {{#if_eq css "less"}}
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    {{/if_eq}}
    {{#if_eq css "sass"}}
    "node-sass": "^4.0.0",
    "sass-loader": "^4.1.0",
    {{/if_eq}}
    {{#if_eq css "stylus"}}
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    {{/if_eq}}
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}